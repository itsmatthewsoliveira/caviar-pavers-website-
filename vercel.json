{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/services", "destination": "/services.html" },
    { "source": "/process", "destination": "/process.html" },
    { "source": "/portfolio", "destination": "/portfolio.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/pavers", "destination": "/pavers.html" },
    { "source": "/pool-decks", "destination": "/pool-decks.html" },
    { "source": "/turf", "destination": "/turf.html" },
    { "source": "/screen-enclosures", "destination": "/screen-enclosures.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ]
}
